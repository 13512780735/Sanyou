<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.likeits.sanyou.ui.shop.AddressSelectActivity">

    <include
        android:id="@+id/titlelayout"
        layout="@layout/layout_header_with_back" />

    <ListView
        android:id="@+id/addressselect_listview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/titlelayout"
        android:divider="@color/line_color"
        android:dividerHeight="1dip"
        android:padding="10dip"></ListView>

    <ListView
        android:id="@+id/addressselect_listview2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/titlelayout"
        android:divider="@color/line_color"
        android:dividerHeight="1dip"
        android:padding="10dip"
        android:visibility="gone"></ListView>

    <ListView
        android:id="@+id/addressselect_listview3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/titlelayout"
        android:divider="@color/line_color"
        android:dividerHeight="1dip"
        android:padding="10dip"
        android:visibility="gone"></ListView>
</LinearLayout>
